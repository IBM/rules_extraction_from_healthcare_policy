<http://claims-audit.ibm.com/> rdf:type owl:Ontology ;
                                owl:versionInfo "[1.3.29] - 22/11/2020"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://claims-audit.ibm.com/affirmativeAction
:affirmativeAction rdf:type owl:AnnotationProperty ;
                   rdfs:comment "Represent an action that can help in identifying the property in the text if found with affirmative polarity. Should contain a verb in its base form, e.g. 'require', 'be eligible'" ;
                   rdfs:range xsd:string .


###  http://claims-audit.ibm.com/annotatedAs
:annotatedAs rdf:type owl:AnnotationProperty ;
             rdfs:comment "Defines the annotations types that correspond to candidate values for the class" ;
             rdfs:range xsd:string .


###  http://claims-audit.ibm.com/claimValue
:claimValue rdf:type owl:AnnotationProperty ;
            rdfs:comment "The value the annotated entity is identified with in the claims"^^xsd:string ;
            rdfs:range xsd:string .


###  http://claims-audit.ibm.com/defaultValue
:defaultValue rdf:type owl:AnnotationProperty ;
              rdfs:comment "This annotation property specifies the default value for properties." .


###  http://claims-audit.ibm.com/displayName
:displayName rdf:type owl:AnnotationProperty ;
             rdfs:comment "A user friendly name that identifies the resource"^^xsd:string ;
             rdfs:range xsd:string .


###  http://claims-audit.ibm.com/extractionPattern
:extractionPattern rdf:type owl:AnnotationProperty ;
                   rdfs:comment "Represent an extraction pattern, i.e. regular expression with placeholders, for the specific resource" ;
                   rdfs:range xsd:string .


###  http://claims-audit.ibm.com/hasComplianceRuleTag
:hasComplianceRuleTag rdf:type owl:AnnotationProperty ;
                      rdfs:range :ComplianceRuleTag ;
                      rdfs:domain :BenefitRule .


###  http://claims-audit.ibm.com/hasConfidence
:hasConfidence rdf:type owl:AnnotationProperty ;
               rdfs:comment "Represent the information regarding the confidence of the domain resource" ;
               rdfs:range :ConfidenceScore .


###  http://claims-audit.ibm.com/hasModifierCode
:hasModifierCode rdf:type owl:AnnotationProperty ;
                 rdfs:range xsd:string ;
                 rdfs:domain :ServiceModifier .


###  http://claims-audit.ibm.com/hasUmlsConceptIdentifier
:hasUmlsConceptIdentifier rdf:type owl:AnnotationProperty ;
                          rdfs:comment "The UMLS entity identifier (CUI)"^^xsd:string ;
                          rdfs:range xsd:string ;
                          rdfs:domain :UmlsConcept .


###  http://claims-audit.ibm.com/hasUmlsPreferredName
:hasUmlsPreferredName rdf:type owl:AnnotationProperty ;
                      rdfs:comment "The preferred resource name in UMLS"^^xsd:string ;
                      rdfs:range xsd:string ;
                      rdfs:domain :UmlsConcept .


###  http://claims-audit.ibm.com/isNegated
:isNegated rdf:type owl:AnnotationProperty ;
           rdfs:comment "If true means that a property has a negative intent, e.g. hasNotApplicableService, hasNoPriorRequirement" ;
           rdfs:range xsd:boolean .


###  http://claims-audit.ibm.com/negativeAction
:negativeAction rdf:type owl:AnnotationProperty ;
                rdfs:comment "Represent an action that can help in identifying the property in the text if found with negative polarity. Should contain a verb in its base form, e.g. 'require', 'be eligible'" ;
                rdfs:range xsd:string .


###  http://claims-audit.ibm.com/nodeId
:nodeId rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/source
dc:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://claims-audit.ibm.com/Date
:Date rdf:type rdfs:Datatype ;
      :annotatedAs "DateTime"^^xsd:string ;
      rdfs:comment "Represents a date in a non specified format, default YYYY-MM-DD"^^xsd:string ;
      owl:equivalentClass xsd:string .


###  http://claims-audit.ibm.com/MonetaryAmount
:MonetaryAmount rdf:type rdfs:Datatype ;
                :annotatedAs "CurrencyAmount" ,
                             "com.ibm.watsonx.nlp_di.hutt.Money" ;
                rdfs:comment "Represents an amount of money with (optional) currency symbol, e.g. 1,000$, â‚¬ 100, 10.00" ;
                owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                      owl:onDatatype xsd:string ;
                                      owl:withRestrictions ( [ xsd:pattern "(\\p{Sc}\\s*){0,1}(\\d+[\\d,]*\\.?\\d*){1}(\\s*\\p{Sc}){0,1}"
                                                             ]
                                                           )
                                    ] .


###  http://www.w3.org/2001/XMLSchema#int
xsd:int :annotatedAs "IntegerNumber" .


#################################################################
#    Object Properties
#################################################################

###  http://claims-audit.ibm.com/belongsToHighRiskCategory
:belongsToHighRiskCategory rdf:type owl:ObjectProperty ;
                           rdfs:domain :MemberEligibility ;
                           rdfs:range :HighRiskCategory ;
                           :affirmativeAction "determine" ;
                           :displayName "member - high risk category"^^xsd:string ;
                           rdfs:label "high risk" .


###  http://claims-audit.ibm.com/hasAge
:hasAge rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :MemberEligibility ;
        rdfs:range :AgeRange ;
        rdfs:label "age" .


###  http://claims-audit.ibm.com/hasApplicableService
:hasApplicableService rdf:type owl:ObjectProperty ;
                      rdfs:domain :ServiceLimitationBenefitRule ;
                      rdfs:range :Service ;
                      owl:propertyDisjointWith :hasExcludedService ;
                      :affirmativeAction "allow" ,
                                         "apply" ,
                                         "be a benefit" ,
                                         "be a cover" ,
                                         "be a payable" ,
                                         "be only a cover" ,
                                         "be payable" ,
                                         "be reimbursable" ,
                                         "be subject" ,
                                         "be the only acceptable" ,
                                         "cover" ,
                                         "include" ,
                                         "limit" ,
                                         "provide" ,
                                         "receive" ,
                                         "reimburse" ;
                      :displayName "applicable service" ;
                      rdfs:comment "Services applicable in the context of the benefit rule, e.g. the ones a limitation is applied to" ;
                      rdfs:label "apply" ,
                                 "cover" ,
                                 "cover benefit"^^xsd:string ,
                                 "cover service"^^xsd:string ,
                                 "include" ,
                                 "limit" ,
                                 "payable" ,
                                 "provide" ,
                                 "reimburse" .


###  http://claims-audit.ibm.com/hasApplicableTimePeriod
:hasApplicableTimePeriod rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( :MutualExclusionBenefitRule
                                                     :NonCoverageBenefitRule
                                                     :ServiceLimitationBenefitRule
                                                   )
                                     ] ;
                         rdfs:range :ApplicableTimePeriod ;
                         rdfs:label "after" ,
                                    "limit" ,
                                    "period"^^xsd:string ,
                                    "provide"^^xsd:string .


###  http://claims-audit.ibm.com/hasBenefitRule
:hasBenefitRule rdf:type owl:ObjectProperty ;
                rdfs:domain :Policy ;
                rdfs:range :BenefitRule ;
                rdfs:comment "A policy may describe multiple instances of benefit rules" .


###  http://claims-audit.ibm.com/hasBillingCommonality
:hasBillingCommonality rdf:type owl:ObjectProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :MutualExclusionBenefitRule
                                                   :ServiceLimitationBenefitRule
                                                 )
                                   ] ;
                       rdfs:range :BillingCommonality ;
                       :displayName "billing commonality" .


###  http://claims-audit.ibm.com/hasExcludedService
:hasExcludedService rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :NonCoverageBenefitRule
                                                :ServiceLimitationBenefitRule
                                              )
                                ] ;
                    rdfs:range :Service ;
                    owl:propertyDisjointWith :hasNotApplicableService ;
                    :affirmativeAction "except" ,
                                       "exclude" ;
                    :displayName "excluded service" ;
                    :isNegated "true"^^xsd:boolean ;
                    :negativeAction "allow" ,
                                    "be not subject" ,
                                    "include" ;
                    rdfs:comment "The services that have to be removed from the set of services relevant for the benefit rule" ;
                    rdfs:label "except" ,
                               "exclude" ,
                               "not include"^^xsd:string ,
                               "not subject"^^xsd:string .


###  http://claims-audit.ibm.com/hasMedicalHistoryOfDisease
:hasMedicalHistoryOfDisease rdf:type owl:ObjectProperty ;
                            rdfs:domain :MemberEligibility ;
                            rdfs:range :UmlsDiseaseOrSyndrome ;
                            owl:propertyDisjointWith :hasNoMedicalHistoryOfDisease ;
                            :affirmativeAction "establish" ;
                            :displayName "member - medical history of disease"^^xsd:string ;
                            rdfs:label "history" ,
                                       "with a history of" .


###  http://claims-audit.ibm.com/hasMedicalHistoryOfTreatment
:hasMedicalHistoryOfTreatment rdf:type owl:ObjectProperty ;
