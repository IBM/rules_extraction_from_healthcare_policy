#################################################################
#    Classes
#################################################################

###  http://claims-audit.ibm.com/AgeRange
:AgeRange rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty owl:sameAs ;
                            owl:someValuesFrom :UmlsPopulationGroup
                          ] ;
          :extractionPattern "(?:through|under|up to) <hasMaxAge>\\s*(?:years|year)\\s*(?:of age){0,1}"^^xsd:string ,
                             "(?:up to|under)\\s*(?:the){0,1}\\s*age[s]*\\s*(?:of){0,1} <hasMaxAge>"^^xsd:string ,
                             "age[s]* <hasMaxAge> (?:years|year){0,1}\\s*(?:old){0,1}\\s*(?:and|or)\\s+(?:under|younger)"^^xsd:string ,
                             "age[s]* <hasMinAge> (?:years|year){0,1}\\s*(?:old){0,1}\\s*(?:and|or)\\s+(?:over|older)"^^xsd:string ,
                             "age[s]* <hasMinAge> through <hasMaxAge>\\s*(?:years|year){0,1}"^^xsd:string ,
                             "through age[s]* <hasMaxAge>"^^xsd:string .


###  http://claims-audit.ibm.com/ApplicableTimePeriod
:ApplicableTimePeriod rdf:type owl:Class ;
                      :extractionPattern "(?:every|within) <hasAmountOfTime>\\s+(?:\\(\\d+\\)\\s*){0,1}<hasUnitOfTime>"^^xsd:string ,
                                         "<hasAmountOfTime>\\s*[-]{0,1}<hasUnitOfTime> (?:interval|period)"^^xsd:string ,
                                         "<hasStartDate> through <hasEndDate>"^^xsd:string ,
                                         "<hasStartDate>\\s*[-]{0,1}\\s*<hasEndDate>"^^xsd:string ;
                      rdfs:comment "The time period the rule is referring to" ;
                      rdfs:label "interval" ,
                                 "lifespan" ,
                                 "period" ,
                                 "when" .


